    "def recur(n):\n",
    "    if n > 0:\n",
    "        recur(n - 2)\n",
    "        print(n)\n",
    "        recur(n - 1)\n",
    "        \n",
    "x = int(input(\"정숫값을 입력하세요.: \"))\n",
    "recur(x)"
